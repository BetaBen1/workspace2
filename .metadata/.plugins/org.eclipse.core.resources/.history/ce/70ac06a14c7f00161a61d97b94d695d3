package betaben.mediaLib;

public class ArrayMediaLib {

	public static void main(String[] args) {
		
		String[] sharingFriends = {new String("Douglas"),
								   new String("Patricia"),
								   new String("Harrison"),
								   new String("Ben"),
								   new String("Brandon"),
								   new String("Adam")
		}
		
		for (int i = 0; i < 5; i++)
		{
		    System.out.println(sharingFriends[i]);
		}


	}

}
